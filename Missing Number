
#include<iostream>
using namespace std;
int main ()
{
  long long n,i;
  cin >> n;
  int a[n-1],b[n];
  
  for(i=0;i<n-1;i++){
      cin>>a[i];
      
  }
  
  for(i=0;i<n;i++){
      b[i]=0;
      
  }
  
  for(i=0;i<n-1;i++){
      int p=a[i] - 1;
      b[p]=1;
      
  }
  
   for(i=0;i<n;i++){
     if(b[i]==0)
     {
     cout<<i+1;
     }
      
  }
  
  
  return 0;
}
