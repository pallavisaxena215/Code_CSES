class Solution {
public:
    vector<int> twoSum(vector<int>& a, int target) {
        
        vector<int>p;
        
        
        
         for(int i=0;i<a.size();i++)
    {
        for(int j=i+1;j<a.size();j++){
            if((a[i]+a[j])==target)
            {
                p.push_back(i);
                p.push_back(j);
                
            } 
        }
    }
    
    return p;

        
    }
};
